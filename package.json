{
  "name": "wasabi",
  "version": "0.0.0",
  "private": true,
  "author": "Jauvat Fabrice",
  "main": "./app.js",
  "babel": {
    "presets": [
      "env",
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "nodemon bin/www --exec babel-node --presets es2015 --ignore mongo/",
    "debug": "babel-node --inspect ./bin/www --exec babel-node --presets es2015",
    "test": "mocha --require babel-polyfill --compilers js:babel-register",
    "apidoc": "apidoc -i routes/ -o apidoc/",
    "start-pm2 ": "pm2 start --interpreter babel-node bin/www --watch --ignore-watch='mongo node_modules' --log-date-format='YYYY-MM-DD HH:mm Z' -f",
    "start-cluster-pm2": "pm2 start --interpreter babel-node bin/www -i max",
    "kill-pm2": "pm2 kill",
    "log-pm2": "pm2 logs --log-date-format='YYYY-MM-DD HH:mm Z' --lines 1000",
    "postinstall": "node ./conf/quickfix.js"
  },
  "dependencies": {
    "assert": "^1.5.0",
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "basic-auth-connect": "^1.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "cheerio": "^0.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.3.5",
    "coreapi": "^0.1.1",
    "csv-parse": "^1.3.3",
    "debug": "~2.2.0",
    "elasticsearch": "^12.1.3",
    "errorhandler": "^1.5.1",
    "event-stream": "^3.3.5",
    "express": "^4.17.1",
    "express-rate-limit": "^2.14.2",
    "express-session": "^1.16.2",
    "helmet": "^2.1.0",
    "http-proxy": "^1.18.0",
    "jsonwebtoken": "^7.3.0",
    "languagedetect": "^1.2.0",
    "levenshtein": "^1.0.5",
    "line-reader": "^0.4.0",
    "mkdirp": "^0.5.1",
    "mongodb": "^2.2.36",
    "mongoose": "^4.13.19",
    "mongoskin": "^2.0.3",
    "morgan": "~1.6.1",
    "mv": "^2.1.1",
    "paper-audio-player": "0.0.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "q": "^1.5.1",
    "request": "2.72.0",
    "serve-favicon": "~2.3.0",
    "sparql": "^0.1.3",
    "spdy": "^4.0.1",
    "tor-request": "^1.2.2",
    "video-url-inspector": "^1.1.0",
    "xml": "^1.0.1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {}
}
